{
  "name": "@tprk77/screeps",
  "version": "1.0.0",
  "description": "Tim's Screeps Code",
  "author": "Tim Perkins",
  "license": "MIT",
  "repository": "github:tprk77/screeps",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json \"src/**/*.ts\"",
    "format": "clang-format -i src/*.ts src/**/*.ts",
    "build": "rollup -c",
    "upload": "rollup -c --environment DEST:main",
    "uploadw": "rollup -cw --environment DEST:main",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/lodash": "^3.10.1",
    "@types/screeps": "^2.4.0",
    "clang-format": "^1.2.3",
    "rollup": "^0.63.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-screeps": "^0.1.2",
    "rollup-plugin-typescript2": "^0.16.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
